{
  "name": "wikibase/elastic",
  "type": "library",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/filbertkm/wikibase-store.git"
    }
  ],
  "require": {
    "ruflin/elastica": "1.3.0.0",
    "wikibase/data-model": "~2.4",
    "wikibase/store": "dev-master"
  },
  "autoload": {
    "psr-4" : {
      "Wikibase\\Elastic\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "autoload-dev": {
    "psr-4": {
      "Wikibase\\Elastic\\Test\\": "tests/"
    }
  }
}
